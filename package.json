{
  "name": "mimetext",
  "version": "v3.0.5",
  "description": "RFC 2822 compliant raw email message generator written in node.js",
  "type": "module",
  "imports": {
    "#src/*": "./build/*"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/es/index.js",
        "require": "./dist/node/cjs/index.js"
      },
      "import": "./dist/browser/es/index.js",
      "require": "./dist/browser/cjs/index.js"
    },
    "./browser": {
      "import": "./dist/browser/es/index.js",
      "require": "./dist/browser/cjs/index.js"
    },
    "./node": {
      "import": "./dist/node/es/index.js",
      "require": "./dist/node/cjs/index.js"
    },
    "./gas": {
      "import": "./dist/gas/es/index.js",
      "require": "./dist/node/cjs/index.js"
    }
  },
  "main": "./dist/node/cjs/index.js",
  "module": "./dist/node/es/index.js",
  "types": "types",
  "scripts": {
    "precompile": "rm -rf build/*",
    "compile": "tsc -p tsconfig.json",
    "build": "rollup --config rollup.config.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "docs": "typedoc --includeVersion --excludePrivate src/entrypoints/node.ts src/entrypoints/gas.ts src/entrypoints/browser.ts types/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/MIMEText.git"
  },
  "keywords": [
    "rfc-2822",
    "mime-message",
    "email-message"
  ],
  "author": "Murat GÃ¶zel <murat@gozel.com.tr>",
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/muratgozel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/MIMEText/issues"
  },
  "homepage": "https://github.com/muratgozel/MIMEText#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/google-apps-script": "^1.0.59",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "aws-sdk": "^2.1341.0",
    "core-js": "^3.18.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "node-releaser": "^2.0.0-beta.4",
    "rollup": "^3.19.1",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "@tsconfig/esm": "^1.0.2",
    "@tsconfig/node18": "^1.0.1",
    "@tsconfig/strictest": "^2.0.0",
    "js-base64": "^3.7.5",
    "mime-types": "^2.1.35"
  },
  "overrides": {
    "xml2js": "0.5.0"
  }
}